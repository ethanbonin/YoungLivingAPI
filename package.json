{
  "name": "younglivingapi",
  "version": "1.0.0",
  "description": "A connection Interface between YoungLiving and your own app.",
  "main": "index.js",
  "scripts": {
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "start": "node server/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "engines": {
    "node": "9.2.0",
    "npm": "5.3.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "client-sessions": "^0.8.0",
    "concurrently": "^3.5.0",
    "cron": "^1.3.0",
    "csvjson": "^4.3.4",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "fwk": "^1.1.8",
    "input-moment": "^0.4.0",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "moment-timezone": "^0.5.14",
    "mongo": "^0.1.0",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.14",
    "path-parser": "^2.0.2",
    "remove": "^0.1.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "requestify": "^0.2.5",
    "resolve-from": "^4.0.0",
    "strip": "^3.0.0",
    "twilio": "^3.9.3",
    "validator": "^8.1.0"
  },
  "devDependencies": {
    "expect": "^1.20.2",
    "install": "^0.10.1",
    "mocha": "^3.5.0",
    "npm": "^5.4.0",
    "supertest": "^3.0.0"
  }
}
