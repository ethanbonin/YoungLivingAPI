{
  "name": "younglivingapi",
  "version": "1.0.0",
  "description": "A connection Interface between YoungLiving and your own app.",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "csvjson": "^4.3.4",
    "express": "^4.15.4",
    "fwk": "^1.1.8",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "mongo": "^0.1.0",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.13",
    "path-parser": "^2.0.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "requestify": "^0.2.5",
    "strip": "^3.0.0",
    "validator": "^8.1.0"
  },
  "devDependencies": {
    "expect": "^1.20.2",
    "install": "^0.10.1",
    "mocha": "^3.5.0",
    "npm": "^5.4.0",
    "supertest": "^3.0.0"
  }
}
